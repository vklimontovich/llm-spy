{
  "name": "otel-proxy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3441",
    "build": "npm run proto:update && prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "proto:fetch": "mkdir -p src/generated && rm -rf .tmp-proto && git clone --depth 1 --branch v1.7.0 https://github.com/open-telemetry/opentelemetry-proto.git .tmp-proto && cp -r .tmp-proto/opentelemetry src/generated/ && rm -rf .tmp-proto",
    "proto:generate": "pbjs -t static-module -w commonjs -p src/generated -o src/generated/otel-proto.js 'src/generated/opentelemetry/proto/**/*.proto' && pbts -o src/generated/otel-proto.d.ts src/generated/otel-proto.js",
    "proto:update": "npm run proto:fetch && npm run proto:generate"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@ant-design/nextjs-registry": "^1.0.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/otlp-exporter-base": "^0.202.0",
    "@opentelemetry/otlp-transformer": "^0.201.1",
    "@opentelemetry/resources": "^2.0.1",
    "@prisma/client": "^6.8.2",
    "@tanstack/react-query": "^5.79.0",
    "@types/pako": "^2.0.3",
    "antd": "^5.25.3",
    "dayjs": "^1.11.13",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "next-auth": "^4.24.11",
    "pako": "^2.1.0",
    "prisma": "^6.8.2",
    "protobufjs": "^7.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "@microlink/react-json-view": "^1.23.4",
    "yaml": "^2.3.4",
    "type-fest": "^4.41.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "prettier": "^3.5.3",
    "protobufjs-cli": "^1.1.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
